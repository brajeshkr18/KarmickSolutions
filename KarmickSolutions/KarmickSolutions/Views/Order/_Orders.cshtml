@model PagedList.IPagedList<DemoModel.CartModel.PlacedOrderViewModel>
    @using PagedList.Mvc;
    @{
    Layout = null;
    }
    <table id="zero_config" class="table table-striped table-bordered">
        <thead>
            <tr>

                <th>Shipping Id</th>
                <th>Items</th>
                <th>Amount</th>
                <th>Payment Mode</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Order Date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (DemoModel.CartModel.PlacedOrderViewModel entity in Model)
            {
            <tr>
                <td>@entity.ShippingDetailId</td>
                <td>@entity.Items</td>
                <td>@entity.Amount</td>
                <td>@entity.PaymentType</td>
                <td>@entity.Email</td>
                <td>@entity.PhoneNumber</td>
                <td>@entity.CreatedOn</td>
                <td>

                    <button class="btn btn-outline-info btn-xs linkbutton" onclick="OpenPopup()">
                        <i class="fa fa-edit"></i>Edit
                    </button>&nbsp;

                    <button class="btn btn-outline-info btn-xs linkbutton" onclick="ViewData(@entity.ShippingDetailId)">
                        <i class="fas fa-eye"></i>View
                    </button>&nbsp;


                </td>
            </tr>
            }
        </tbody>
     

    </table>

   
