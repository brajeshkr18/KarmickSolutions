@model PagedList.IPagedList<DemoModel.ViewModel.BookingReqestViewModel>
@using PagedList.Mvc;
@{
    Layout = null;
}
<table id="zero_config" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Name</th>
            <th>PhoneNo</th>
            <th>Booking for Date</th>
            <th>Time</th>
            <th>People</th>
            <th>Created Date</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (DemoModel.ViewModel.BookingReqestViewModel entity in Model)
        {
        <tr>
            <td>@entity.Id</td>
            <td>@entity.Email</td>
            <td>@entity.Name</td>
            <td>@entity.PhoneNo</td>
            <td>@entity.Date.ToShortDateString()</td>
            <td>@entity.Time</td>
            <td>@entity.People</td>
            <td>@entity.CreatedDate</td>
            <td>
                @*<a class="btn btn-outline-info btn-xs linkbutton"
            href="@Url.Action("AddBookings", "BookingRequest"
             ,new {data=entity.Id})" name="edit"><i class="fa fa-edit"></i>Edit</a>&nbsp;*@

            </td>
        </tr>
        }
    </tbody>
    <tfoot>
       
    </tfoot>

</table>

@*<script src="~/AdminAsset/assets/extra-libs/DataTables/datatables.min.js"></script>*@
