@model PagedList.IPagedList<DemoModel.ViewModel.PaymentViewModel>
@using PagedList.Mvc;
@{
    Layout = null;
}

<table id="zero_config" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ShippingDetailId</th>
            <th>MemberId</th>
            <th>AmountPaid</th>
            <th>Email</th>
            <th>Response</th>

            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (DemoModel.ViewModel.PaymentViewModel entity in Model)
        {
        <tr>
            <td>@entity.ShippingDetailId</td>
            <td>@entity.MemberId</td>
            <th>@entity.AmountPaid</th>
            <th>@entity.Email</th>
            <td>@entity.Response</td>

            <td>@*<img src="@entity.GalleryPath" style="width: 150px;" />*@</td>

        </tr>
                }
        </tbody>
       
    </table>

@*<script src="~/AdminAsset/assets/extra-libs/DataTables/datatables.min.js"></script>*@
<script>
    /****************************************
     *       Basic Table                   *
     ****************************************/
    $('#zero_config').DataTable();
</script>